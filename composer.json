{
    "name": "vysmv/easy-start",
    "description": "Quickly set up a Laravel development environment with Docker (nginx, php-fpm, mysql, redis, minio).",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Suhomlinov Vitaliy",
            "email": "scriptcraft.site@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0 || ^12.0"
    },
    "autoload": {
        "psr-4": {
            "Vysmv\\EasyStart\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Vysmv\\EasyStart\\Core\\EasyStartCoreServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
